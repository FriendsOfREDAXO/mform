{
    "name": "redaxo/php-cs-fixer-config",
    "description": "php-cs-fixer config for REDAXO",
    "type": "library",
    "keywords": [
        "fixer",
        "standards",
        "static analysis",
        "static code analysis"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "REDAXO Team",
            "email": "info@redaxo.org"
        }
    ],
    "require": {
        "php": "^8.1",
        "friendsofphp/php-cs-fixer": "^3.17.0",
        "kubawerlos/php-cs-fixer-custom-fixers": "^3.14.0"
    },
    "autoload": {
        "psr-4": {
            "Redaxo\\PhpCsFixerConfig\\": "src"
        }
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    }
}
